<div class="col-lg-12 mb-4">

  <%= render 'layouts/notice' %>

  <div class="card shadow mb-4">
    <div class="card-header py-3">
      <h3 class="m-0 font-weight-bold text-primary">
        Crews for <%= link_to "Squad #{@squad.name}", mission_squad_path(@mission, @squad), target: :_blank %>
      </h3>
    </div>
    
    <%= render partial:'crew_list', 
               locals: { mission: @mission, squad: @squad, crews: @crews } %>

  </div>
</div>
